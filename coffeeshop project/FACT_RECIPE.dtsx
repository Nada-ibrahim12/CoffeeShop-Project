    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        // Extract NTEXT (BlobColumn) to string
        int ingLength = (int)Row.ingredients.Length;
        byte[] ingData = Row.ingredients.GetBlobData(0, ingLength);
        string ingredientsStr = System.Text.Encoding.Unicode.GetString(ingData);
        string[] ingArray = ingredientsStr.Split(',');

        int qtyLength = (int)Row.quantities.Length;
        byte[] qtyData = Row.quantities.GetBlobData(0, qtyLength);
        string quantitiesStr = System.Text.Encoding.Unicode.GetString(qtyData);
        string[] qtyArray = quantitiesStr.Split(',');

        string item_id = Row.itemid.ToString();
        int item_price = (int)Row.itemprice;

        for (int i = 0; i < ingArray.Length; i++)
        {
            Output0Buffer.AddRow();
            Output0Buffer.recipe = Row.OLEDBSource2recipeid.ToString();
            Output0Buffer.ing = ingArray[i].Trim();

            decimal parsedQuantity = 0; // default value
            if (qtyArray.Length > i)
            {
                string qtyValue = qtyArray[i].Trim();

                // Check if string is not empty/null before parsing
                if (!string.IsNullOrWhiteSpace(qtyValue))
                {
                    // Try parsing the quantity
                    if (!decimal.TryParse(qtyValue, System.Globalization.NumberStyles.Any, System.Globalization.CultureInfo.InvariantCulture, out parsedQuantity))
